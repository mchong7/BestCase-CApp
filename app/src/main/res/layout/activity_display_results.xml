 <TextView
       android:id="@+id/results"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentTop="true"
       android:layout_centerHorizontal="true"
       android:layout_gravity="center_horizontal|top"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:layout_weight="1"
       android:fontFamily="sans-serif"
       android:text="@string/results"
       android:textAlignment="center"
       android:textAppearance="@android:style/TextAppearance.Material.Large" />
 <TextView
       android:id="@+id/velocity_0_num"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_below="@+id/results"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:text="Initial Vel"
       android:textAlignment="textStart"
       android:textAppearance="?android:attr/textAppearanceLarge" />
 <TextView
       android:id="@+id/velocity_num"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignStart="@+id/velocity_0_num"
       android:layout_below="@+id/velocity_0_num"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:text="velocity"
       android:textAlignment="textStart"
       android:textAppearance="?android:attr/textAppearanceLarge" />
 <TextView
       android:id="@+id/accel_num"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_below="@id/velocity_num"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:text="Accel"
       android:textAlignment="textStart"
       android:textAppearance="?android:attr/textAppearanceLarge" />
 <TextView
       android:id="@+id/time_num"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignStart="@+id/accel_num"
       android:layout_below="@id/accel_num"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:text="time"
       android:textAlignment="textStart"
       android:textAppearance="?android:attr/textAppearanceLarge" />
 <TextView
       android:id="@+id/displace_num"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignEnd="@+id/time_num"
       android:layout_below="@+id/time_num"
       android:layout_marginBottom="@dimen/activity_vertical_margin"
       android:text="disp"
       android:textAlignment="textStart"
       android:textAppearance="?android:attr/textAppearanceLarge" />
 <Button
       android:id="@+id/back"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentBottom="true"
       android:layout_alignParentEnd="true"
       android:text="BACK" />

 <Button
     android:id="@+id/explain"
     style="@android:style/Widget.DeviceDefault.Button.Borderless.Small"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_below="@+id/displace_num"
     android:layout_centerHorizontal="true"
     android:layout_marginTop="33dp"
     android:elevation="0dp"
     android:text="Explain Answers" />
