<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bestcase.capp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CApp Physics Helper"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashPage" android:screenOrientation="portrait">

            <!-- android:label="@string/CApp"> -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DataInput" android:screenOrientation="portrait">

            <!-- android:label="@string/CApp"> -->
            <intent-filter>
                <action android:name="android.intent.action.MAINLAUNCHER" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayResults" android:screenOrientation="portrait">


        </activity>
    </application>

</manifest>